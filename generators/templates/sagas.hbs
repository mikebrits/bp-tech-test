{{#if actions}} import { defaultAction } from './{{properCase name}}.actions'{{/if}}

export const defaultSaga = function*() {
    {{#if actions}} yield put(defaultAction);{{/if}}
};

export default [takeEvery( {{#if constants}} {{constantCase name}} {{else}} '{{constantCase name}}'{{/if}}, defaultSaga)];


